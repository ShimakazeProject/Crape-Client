<Application x:Class="Crape_Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Crape_Client"
             StartupUri="CrapeClientUI/Crape Client.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--  如果你用到DMSkin窗体需要导入  -->
                <ResourceDictionary Source="pack://application:,,,/DMSkin;component/DMSkin.xaml" />
                <!--  AduWindow  -->
                <ResourceDictionary Source="pack://application:,,,/DMSkin.AduDesign;component/Styles/AduWindow.xaml" />
                <!--  引用目标设计库的Style  -->
                <ResourceDictionary Source="pack://application:,,,/DMSkin.AduDesign;component/Style.xaml" />
                
                <!--  自己的库  -->
                <ResourceDictionary Source="pack://application:,,,/Crape Client;component/CrapeClientUI/LocalDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
